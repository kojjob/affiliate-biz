<% content_for :title, "About Us | Affiliate Biz" %>
<% content_for :meta_description, "Learn about Affiliate Biz - our mission, values, team, and journey to becoming the leading affiliate marketing platform." %>

<div data-controller="about-page" class="bg-gradient-to-b from-indigo-50 to-white">
  <!-- Hero Section -->
  <section class="relative overflow-hidden pb-12 pt-32 sm:pb-16 sm:pt-40">
    <div class="absolute inset-0 bg-[url('/hero-pattern.svg')] bg-center [mask-image:linear-gradient(180deg,white,rgba(255,255,255,0))]"></div>

    <!-- Animated background elements -->
    <div class="absolute top-1/4 left-1/4 w-72 h-72 bg-purple-300 rounded-full mix-blend-multiply filter blur-3xl opacity-20 animate-blob"></div>
    <div class="absolute top-1/3 right-1/4 w-72 h-72 bg-yellow-300 rounded-full mix-blend-multiply filter blur-3xl opacity-20 animate-blob animation-delay-2000"></div>
    <div class="absolute bottom-1/4 right-1/3 w-72 h-72 bg-pink-300 rounded-full mix-blend-multiply filter blur-3xl opacity-20 animate-blob animation-delay-4000"></div>

    <div class="mx-auto max-w-7xl px-6 lg:px-8">
      <div class="relative z-10 mx-auto max-w-2xl text-center">
        <span class="inline-flex items-center rounded-md bg-indigo-50 px-2 py-1 text-xs font-medium text-indigo-700 ring-1 ring-inset ring-indigo-700/10 mb-4">Established 2020</span>
        <h1 class="animate-fade-in bg-gradient-to-r from-indigo-600 via-purple-500 to-pink-500 bg-clip-text text-4xl font-bold tracking-tight text-transparent sm:text-6xl">
          Our Story
        </h1>
        <p class="mt-6 text-lg leading-8 text-gray-600">
          Discover how we're revolutionizing affiliate marketing with transparency, quality products, and innovative technology.
        </p>
        <div class="mt-10 flex justify-center gap-x-6">
          <a href="#history" class="text-sm font-semibold leading-6 text-indigo-600 border border-indigo-200 rounded-full px-4 py-2 hover:bg-indigo-50 transition-colors" data-action="click->about-page#scrollToSection" data-section="history">
            Our Journey
          </a>
          <a href="#team" class="text-sm font-semibold leading-6 text-indigo-600 border border-indigo-200 rounded-full px-4 py-2 hover:bg-indigo-50 transition-colors" data-action="click->about-page#scrollToSection" data-section="team">
            Meet Our Team
          </a>
        </div>
      </div>
    </div>
  </section>

  <!-- Mission Section -->
  <section class="py-16 sm:py-24">
    <div class="mx-auto max-w-7xl px-6 lg:px-8">
      <div class="mx-auto grid max-w-2xl grid-cols-1 gap-x-8 gap-y-16 sm:gap-y-20 lg:mx-0 lg:max-w-none lg:grid-cols-2 items-center">
        <div>
          <h2 class="text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">Our Mission</h2>
          <p class="mt-6 text-lg leading-8 text-gray-600">
            At Affiliate Biz, we're on a mission to transform the affiliate marketing landscape by creating a platform that benefits everyone involved - from content creators to consumers.
          </p>
          <p class="mt-4 text-lg leading-8 text-gray-600">
            We believe in the power of honest recommendations, quality products, and fair compensation for creators who help connect consumers with products they'll love.
          </p>
          <div class="mt-10 flex items-center gap-x-6">
            <a href="#values" class="text-sm font-semibold leading-6 text-indigo-600" data-action="click->about-page#scrollToSection" data-section="values">
              Our Values <span aria-hidden="true">→</span>
            </a>
          </div>
        </div>
        <div class="relative">
          <img src="<%= asset_path('mission.png') %>" alt="Our mission visualization" class="w-full rounded-xl shadow-xl ring-1 ring-gray-400/10 sm:w-[57rem]" onerror="this.onerror=null; this.src='/api/placeholder/600/400?text=Our+Mission'; this.alt='Our mission';">
          <div class="absolute -top-4 -left-4 w-72 h-72 bg-indigo-50 rounded-full mix-blend-multiply filter blur-3xl opacity-70"></div>
          <div class="absolute -bottom-4 -right-4 w-72 h-72 bg-purple-50 rounded-full mix-blend-multiply filter blur-3xl opacity-70"></div>
        </div>
      </div>
    </div>
  </section>

  <!-- Company History Timeline Section -->
  <section id="history" class="py-16 sm:py-24 bg-gradient-to-r from-indigo-50 to-white">
    <div class="mx-auto max-w-7xl px-6 lg:px-8">
      <div class="mx-auto max-w-2xl lg:text-center">
        <h2 class="text-base font-semibold leading-7 text-indigo-600">Our Journey</h2>
        <p class="mt-2 text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">From Idea to Industry Leader</p>
        <p class="mt-6 text-lg leading-8 text-gray-600">
          Follow our journey from a small startup to becoming a leading platform in the affiliate marketing industry.
        </p>
      </div>

      <div class="mx-auto mt-16 max-w-2xl lg:max-w-4xl">
        <div class="relative" data-about-page-target="timeline">
          <!-- Timeline line -->
          <div class="absolute left-1/2 top-0 bottom-0 w-0.5 bg-indigo-200 transform -translate-x-1/2"></div>

          <!-- Timeline items -->
          <div class="relative z-10">
            <!-- 2020: Founding -->
            <div class="timeline-item mb-16 flex items-center" data-about-page-target="timelineItem">
              <div class="w-1/2 pr-8 text-right">
                <div class="bg-white p-6 rounded-xl shadow-sm border border-gray-100 inline-block">
                  <h3 class="text-lg font-semibold text-gray-900">Company Founded</h3>
                  <time class="block text-sm font-medium text-indigo-600 mb-2">January 2020</time>
                  <p class="text-gray-600">Affiliate Biz was founded with a mission to revolutionize the affiliate marketing industry with transparency and quality.</p>
                </div>
              </div>
              <div class="timeline-marker w-4 h-4 bg-indigo-600 rounded-full border-4 border-white shadow"></div>
              <div class="w-1/2"></div>
            </div>

            <!-- 2020: First Platform -->
            <div class="timeline-item mb-16 flex items-center" data-about-page-target="timelineItem">
              <div class="w-1/2"></div>
              <div class="timeline-marker w-4 h-4 bg-indigo-600 rounded-full border-4 border-white shadow"></div>
              <div class="w-1/2 pl-8">
                <div class="bg-white p-6 rounded-xl shadow-sm border border-gray-100 inline-block">
                  <h3 class="text-lg font-semibold text-gray-900">First Platform Launch</h3>
                  <time class="block text-sm font-medium text-indigo-600 mb-2">June 2020</time>
                  <p class="text-gray-600">We launched our first version of the platform with 50 carefully selected products and 100 beta testers.</p>
                </div>
              </div>
            </div>

            <!-- 2021: Growth -->
            <div class="timeline-item mb-16 flex items-center" data-about-page-target="timelineItem">
              <div class="w-1/2 pr-8 text-right">
                <div class="bg-white p-6 rounded-xl shadow-sm border border-gray-100 inline-block">
                  <h3 class="text-lg font-semibold text-gray-900">Rapid Growth</h3>
                  <time class="block text-sm font-medium text-indigo-600 mb-2">March 2021</time>
                  <p class="text-gray-600">Reached 1,000 active affiliates and expanded our product catalog to over 5,000 items from premium brands.</p>
                </div>
              </div>
              <div class="timeline-marker w-4 h-4 bg-indigo-600 rounded-full border-4 border-white shadow"></div>
              <div class="w-1/2"></div>
            </div>

            <!-- 2022: Funding -->
            <div class="timeline-item mb-16 flex items-center" data-about-page-target="timelineItem">
              <div class="w-1/2"></div>
              <div class="timeline-marker w-4 h-4 bg-indigo-600 rounded-full border-4 border-white shadow"></div>
              <div class="w-1/2 pl-8">
                <div class="bg-white p-6 rounded-xl shadow-sm border border-gray-100 inline-block">
                  <h3 class="text-lg font-semibold text-gray-900">Series A Funding</h3>
                  <time class="block text-sm font-medium text-indigo-600 mb-2">January 2022</time>
                  <p class="text-gray-600">Secured $10M in Series A funding to expand our team and develop advanced analytics tools for our affiliates.</p>
                </div>
              </div>
            </div>

            <!-- 2023: International -->
            <div class="timeline-item mb-16 flex items-center" data-about-page-target="timelineItem">
              <div class="w-1/2 pr-8 text-right">
                <div class="bg-white p-6 rounded-xl shadow-sm border border-gray-100 inline-block">
                  <h3 class="text-lg font-semibold text-gray-900">International Expansion</h3>
                  <time class="block text-sm font-medium text-indigo-600 mb-2">April 2023</time>
                  <p class="text-gray-600">Expanded operations to Europe and Asia, with localized platforms and region-specific product catalogs.</p>
                </div>
              </div>
              <div class="timeline-marker w-4 h-4 bg-indigo-600 rounded-full border-4 border-white shadow"></div>
              <div class="w-1/2"></div>
            </div>

            <!-- 2024: Today -->
            <div class="timeline-item flex items-center" data-about-page-target="timelineItem">
              <div class="w-1/2"></div>
              <div class="timeline-marker w-4 h-4 bg-indigo-600 rounded-full border-4 border-white shadow"></div>
              <div class="w-1/2 pl-8">
                <div class="bg-white p-6 rounded-xl shadow-sm border border-gray-100 inline-block">
                  <h3 class="text-lg font-semibold text-gray-900">Today</h3>
                  <time class="block text-sm font-medium text-indigo-600 mb-2">2024</time>
                  <p class="text-gray-600">Now serving over 5,000 active affiliates with 10,000+ products and expanding into new verticals every month.</p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Values Section -->
  <section id="values" class="py-16 sm:py-24 bg-white">
    <div class="mx-auto max-w-7xl px-6 lg:px-8">
      <div class="mx-auto max-w-2xl lg:text-center">
        <h2 class="text-base font-semibold leading-7 text-indigo-600">Our Foundation</h2>
        <p class="mt-2 text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">Values That Drive Us</p>
        <p class="mt-6 text-lg leading-8 text-gray-600">
          These core principles guide everything we do, from product selection to platform development.
        </p>
      </div>
      <div class="mx-auto mt-16 max-w-2xl sm:mt-20 lg:mt-24 lg:max-w-none">
        <dl class="grid max-w-xl grid-cols-1 gap-x-8 gap-y-16 lg:max-w-none lg:grid-cols-3">
          <div class="flex flex-col" data-about-page-target="valueCard">
            <dt class="flex items-center gap-x-3 text-base font-semibold leading-7 text-gray-900">
              <svg class="h-5 w-5 flex-none text-indigo-600" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                <path fill-rule="evenodd" d="M16.704 4.153a.75.75 0 01.143 1.052l-8 10.5a.75.75 0 01-1.127.075l-4.5-4.5a.75.75 0 011.06-1.06l3.894 3.893 7.48-9.817a.75.75 0 011.05-.143z" clip-rule="evenodd" />
              </svg>
              Transparency
            </dt>
            <dd class="mt-4 flex flex-auto flex-col text-base leading-7 text-gray-600">
              <p class="flex-auto">
                We believe in complete transparency in all our operations. From commission structures to product reviews, we ensure everything is clear and honest.
              </p>
            </dd>
          </div>
          <div class="flex flex-col" data-about-page-target="valueCard">
            <dt class="flex items-center gap-x-3 text-base font-semibold leading-7 text-gray-900">
              <svg class="h-5 w-5 flex-none text-indigo-600" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                <path fill-rule="evenodd" d="M16.704 4.153a.75.75 0 01.143 1.052l-8 10.5a.75.75 0 01-1.127.075l-4.5-4.5a.75.75 0 011.06-1.06l3.894 3.893 7.48-9.817a.75.75 0 011.05-.143z" clip-rule="evenodd" />
              </svg>
              Quality
            </dt>
            <dd class="mt-4 flex flex-auto flex-col text-base leading-7 text-gray-600">
              <p class="flex-auto">
                We rigorously vet all products on our platform to ensure they meet our high standards. We only promote products we believe in and would use ourselves.
              </p>
            </dd>
          </div>
          <div class="flex flex-col" data-about-page-target="valueCard">
            <dt class="flex items-center gap-x-3 text-base font-semibold leading-7 text-gray-900">
              <svg class="h-5 w-5 flex-none text-indigo-600" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                <path fill-rule="evenodd" d="M16.704 4.153a.75.75 0 01.143 1.052l-8 10.5a.75.75 0 01-1.127.075l-4.5-4.5a.75.75 0 011.06-1.06l3.894 3.893 7.48-9.817a.75.75 0 011.05-.143z" clip-rule="evenodd" />
              </svg>
              Innovation
            </dt>
            <dd class="mt-4 flex flex-auto flex-col text-base leading-7 text-gray-600">
              <p class="flex-auto">
                We're constantly pushing the boundaries of what's possible in affiliate marketing, developing new tools and features to help our partners succeed.
              </p>
            </dd>
          </div>
        </dl>
      </div>
    </div>
  </section>

  <!-- Team Section -->
  <section id="team" class="py-16 sm:py-24">
    <div class="mx-auto max-w-7xl px-6 lg:px-8">
      <div class="mx-auto max-w-2xl lg:mx-0">
        <h2 class="text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">Our Team</h2>
        <p class="mt-6 text-lg leading-8 text-gray-600">
          Meet the passionate individuals behind Affiliate Biz who are dedicated to revolutionizing the affiliate marketing industry.
        </p>
      </div>
      <ul role="list" class="mx-auto mt-20 grid max-w-2xl grid-cols-1 gap-x-8 gap-y-16 sm:grid-cols-2 lg:mx-0 lg:max-w-none lg:grid-cols-3">
        <li class="group" data-about-page-target="teamMember">
          <div class="relative overflow-hidden rounded-2xl bg-gray-100 transform transition duration-500 group-hover:scale-105">
            <img class="aspect-[3/2] w-full object-cover" src="<%= asset_path('team-1.png') %>" alt="Team member" onerror="this.onerror=null; this.src='/api/placeholder/300/200?text=CEO'; this.alt='CEO';">
            <div class="absolute inset-0 bg-gradient-to-t from-gray-900 via-gray-900/40 opacity-0 group-hover:opacity-100 transition-opacity duration-300"></div>
            <div class="absolute bottom-0 left-0 right-0 p-6 text-white opacity-0 group-hover:opacity-100 transition-opacity duration-300">
              <p class="text-sm">
                "I founded Affiliate Biz to create a platform that truly values content creators and helps consumers find products they'll love."
              </p>
            </div>
          </div>
          <h3 class="mt-6 text-lg font-semibold leading-8 text-gray-900">Sarah Johnson</h3>
          <p class="text-base leading-7 text-gray-600">CEO & Founder</p>
        </li>
        <li class="group" data-about-page-target="teamMember">
          <div class="relative overflow-hidden rounded-2xl bg-gray-100 transform transition duration-500 group-hover:scale-105">
            <img class="aspect-[3/2] w-full object-cover" src="<%= asset_path('team-2.png') %>" alt="Team member" onerror="this.onerror=null; this.src='/api/placeholder/300/200?text=CTO'; this.alt='CTO';">
            <div class="absolute inset-0 bg-gradient-to-t from-gray-900 via-gray-900/40 opacity-0 group-hover:opacity-100 transition-opacity duration-300"></div>
            <div class="absolute bottom-0 left-0 right-0 p-6 text-white opacity-0 group-hover:opacity-100 transition-opacity duration-300">
              <p class="text-sm">
                "Our technology is built to provide the most transparent and effective affiliate marketing platform in the industry."
              </p>
            </div>
          </div>
          <h3 class="mt-6 text-lg font-semibold leading-8 text-gray-900">Michael Chen</h3>
          <p class="text-base leading-7 text-gray-600">CTO</p>
        </li>
        <li class="group" data-about-page-target="teamMember">
          <div class="relative overflow-hidden rounded-2xl bg-gray-100 transform transition duration-500 group-hover:scale-105">
            <img class="aspect-[3/2] w-full object-cover" src="<%= asset_path('team-3.png') %>" alt="Team member" onerror="this.onerror=null; this.src='/api/placeholder/300/200?text=COO'; this.alt='COO';">
            <div class="absolute inset-0 bg-gradient-to-t from-gray-900 via-gray-900/40 opacity-0 group-hover:opacity-100 transition-opacity duration-300"></div>
            <div class="absolute bottom-0 left-0 right-0 p-6 text-white opacity-0 group-hover:opacity-100 transition-opacity duration-300">
              <p class="text-sm">
                "We're building relationships with the best brands to bring quality products to our platform and our users."
              </p>
            </div>
          </div>
          <h3 class="mt-6 text-lg font-semibold leading-8 text-gray-900">Jessica Rodriguez</h3>
          <p class="text-base leading-7 text-gray-600">COO</p>
        </li>
      </ul>
    </div>
  </section>

  <!-- Testimonials Section -->
  <section class="py-16 sm:py-24 bg-gradient-to-br from-indigo-50 via-white to-purple-50">
    <div class="mx-auto max-w-7xl px-6 lg:px-8">
      <div class="mx-auto max-w-2xl lg:text-center">
        <h2 class="text-base font-semibold leading-7 text-indigo-600">Testimonials</h2>
        <p class="mt-2 text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">What Our Community Says</p>
        <p class="mt-6 text-lg leading-8 text-gray-600">
          Hear from the content creators and brands who have found success with our platform.
        </p>
      </div>

      <div class="mx-auto mt-16 flow-root max-w-2xl lg:mx-0 lg:max-w-none" data-about-page-target="testimonials">
        <!-- Testimonial Slider -->
        <div class="relative overflow-hidden">
          <div class="flex flex-nowrap overflow-x-auto pb-8 snap-x snap-mandatory hide-scrollbar" data-about-page-target="testimonialSlider">
            <!-- Testimonial 1 -->
            <div class="w-full flex-none snap-center px-4 sm:px-6 md:w-1/2 lg:w-1/3" data-about-page-target="testimonialSlide">
              <div class="relative h-full rounded-2xl bg-white p-8 shadow-sm ring-1 ring-gray-900/5">
                <div class="flex items-center gap-4 mb-6">
                  <img class="h-12 w-12 rounded-full object-cover" src="<%= asset_path('testimonial-1.png') %>" alt="Testimonial author" onerror="this.onerror=null; this.src='/api/placeholder/48/48?text=JD'; this.alt='Testimonial author';">
                  <div>
                    <h3 class="text-lg font-semibold text-gray-900">Jessica Davis</h3>
                    <p class="text-sm text-gray-500">Fashion Blogger</p>
                  </div>
                </div>
                <blockquote class="text-gray-700">
                  <p>"Affiliate Biz has transformed my blog's monetization strategy. Their transparent commission structure and high-quality products have helped me increase my revenue by 300% in just six months."</p>
                </blockquote>
                <div class="mt-6 flex items-center text-amber-400">
                  <svg class="h-5 w-5" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                    <path fill-rule="evenodd" d="M10.868 2.884c-.321-.772-1.415-.772-1.736 0l-1.83 4.401-4.753.381c-.833.067-1.171 1.107-.536 1.651l3.62 3.102-1.106 4.637c-.194.813.691 1.456 1.405 1.02L10 15.591l4.069 2.485c.713.436 1.598-.207 1.404-1.02l-1.106-4.637 3.62-3.102c.635-.544.297-1.584-.536-1.65l-4.752-.382-1.831-4.401z" clip-rule="evenodd" />
                  </svg>
                  <svg class="h-5 w-5" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                    <path fill-rule="evenodd" d="M10.868 2.884c-.321-.772-1.415-.772-1.736 0l-1.83 4.401-4.753.381c-.833.067-1.171 1.107-.536 1.651l3.62 3.102-1.106 4.637c-.194.813.691 1.456 1.405 1.02L10 15.591l4.069 2.485c.713.436 1.598-.207 1.404-1.02l-1.106-4.637 3.62-3.102c.635-.544.297-1.584-.536-1.65l-4.752-.382-1.831-4.401z" clip-rule="evenodd" />
                  </svg>
                  <svg class="h-5 w-5" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                    <path fill-rule="evenodd" d="M10.868 2.884c-.321-.772-1.415-.772-1.736 0l-1.83 4.401-4.753.381c-.833.067-1.171 1.107-.536 1.651l3.62 3.102-1.106 4.637c-.194.813.691 1.456 1.405 1.02L10 15.591l4.069 2.485c.713.436 1.598-.207 1.404-1.02l-1.106-4.637 3.62-3.102c.635-.544.297-1.584-.536-1.65l-4.752-.382-1.831-4.401z" clip-rule="evenodd" />
                  </svg>
                  <svg class="h-5 w-5" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                    <path fill-rule="evenodd" d="M10.868 2.884c-.321-.772-1.415-.772-1.736 0l-1.83 4.401-4.753.381c-.833.067-1.171 1.107-.536 1.651l3.62 3.102-1.106 4.637c-.194.813.691 1.456 1.405 1.02L10 15.591l4.069 2.485c.713.436 1.598-.207 1.404-1.02l-1.106-4.637 3.62-3.102c.635-.544.297-1.584-.536-1.65l-4.752-.382-1.831-4.401z" clip-rule="evenodd" />
                  </svg>
                  <svg class="h-5 w-5" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
