<div id="<%= dom_id click %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Affiliate link:</strong>
    <%= click.affiliate_link_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Ip hash:</strong>
    <%= click.ip_hash %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Referrer:</strong>
    <%= click.referrer %>
  </div>
  <div>
    <strong class="block font-medium mb-1">User agent:</strong>
    <%= click.user_agent %>
  </div>
</div>
